<!-- omni_* variables are set by templates in the call to wrapper.tt2 -->

<!-- BEGIN CustomLink Omniture JS -->
<script type="text/javascript">

	var omniture_tag, listProps, common_tags, tag_object, contact_omniture;

	//wrapper function that uses setTimeout in order to create a sep thread so processing the rest of the page isnt interrupted
	omniture_tag = function( obj, item_type, img_file ) {
		setTimeout("contact_omniture(true,'" + item_type + "','" + img_file + "')",0);
	}

	//for debugging
	listProps = function( obj ) {
		var names = "";
		for(var i in obj) names += i + " = " + obj[i] + "<BR>\n";
		document.write("Properties:<BR>" + names);
	}

	//common vars set for carousel/zoom gallery
	common_tags = function( s ) {
		s.linkTrackEvents='event12,event13,event14,event15';  
		s.eVar21=s.eVar25='+1';  
		s.products=';e7f8' 
		return s;
	}

	tag_object = function( obj, img_file, description, evar, linkVarString ) {
		var s = s_gi('thinkgeek'),
			evarObj;
		s = common_tags(s);
		s.prop2 = s.prop3 = s.prop4 = s.prop10 = description;
		s.linkTrackVars = 'prop2,prop3,prop4,prop10,' + linkVarString + ',events,products';
		
		if ( evar == 'eVar22' ) {
			evarObj = s.eVar22 = s.prop10 + ' | Half-Life Headcrab Hat | e7f8 | ' + img_file;
			s.events = 'event12';
		} else if ( evar == 'eVar23' ) {
			evarObj = s.eVar23 = s.prop10 + ' | Half-Life Headcrab Hat | e7f8 | ' + img_file;  
			s.events = 'event13';
		} else if ( evar == 'eVar24' || evar == 'eVar25' ) {
			evarObj = s.eVar24 = s.prop10 + ' | Half-Life Headcrab Hat | e7f8 | ' + img_file;
			s.events = 'event14';  
		}

		s.tl(obj,'o',evarObj);
	}
	
	//main tagging function
	contact_omniture = function( obj, item_type, img_file ) {
		if (item_type == "addtl_thumb") {
			//for selection of additional image thumbnails from carousel
			tag_object( obj, img_file, 'Carousel Select Additional', 'eVar22', 'eVar21,eVar22,eVar25' );
		} else if (item_type == "main_thumb") {
			// for selection of main thumbnail from carousel	
			tag_object( obj, img_file, 'Carousel Select Main', 'eVar22', 'eVar21,eVar22,eVar25' );
		} else if (item_type == "main_zoom") {
			//for FBox zooming of main image	
			tag_object( obj, img_file, 'Carousel Zoom Main', 'eVar23', 'eVar21,eVar23,eVar25' );	
		} else if (item_type == "addtl_zoom") {
			//for FBox zooming of addtl large image	
			tag_object( obj, img_file, 'Carousel Zoom Additional', 'eVar23', 'eVar21,eVar23,eVar25' );
		} else if (item_type == "close_zoom") {
			//for FBox closing zoomed image
			tag_object( obj, img_file, 'Gallery Navigate Close', 'eVar24', 'eVar24' );
		} else if (item_type == "gallery_addtl") {
			//for FBox Gallery navigation TO an additional image FROM a main/addtl gallery image
			tag_object( obj, img_file, 'Gallery Navigate Additional', 'eVar25', 'eVar21,eVar24,eVar25' );
		} else if (item_type == "gallery_main") {
			//for FBox Gallery navigation TO main image FROM an addtl gallery image
			tag_object( obj, img_file, 'Gallery Navigate Main', 'eVar25', 'eVar21,eVar24,eVar25' );
		} else if (item_type == "slider_nav") {
			// for jCarousel next/prev slider navigation
			// in this case, the second param is actually the navigation direction
			var s = s_gi('thinkgeek'),
				evarObj;
			s = common_tags(s);
			s.prop2 = s.prop3 = s.prop4 = s.prop10 = 'Carousel Navigate Thumbnail';
			s.linkTrackVars = 'prop2,prop3,prop4,prop10,eVar22,events,products';
			
			evarObj = s.eVar22 = s.prop10 + ' | Half-Life Headcrab Hat | e7f8 | ' + img_file;
			s.events = 'event15';

			s.tl(obj,'o',evarObj);
		}
	}
	
</script>
<!-- END CustomLink Omniture JS -->

